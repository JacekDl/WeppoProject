<!DOCTYPE html>
<html lang="en">

<%- include ('components/head.ejs', {title: "Warzywniak: Administrator - zamÃ³wienia" , style: "adminOrders.css" })%>

    <body>
        <%- include('components/adminPanel.ejs') %>

            <%- include('components/header.ejs') %>

                <main class="orders__panel">
                    <% for (let i=0; i<orders.length; i++) { %>
                        <div class="panel__item order">
                            <span class="order__username">
                                <%= orders[i].user%>
                            </span>
                            <div class="panel__item orders">
                                <% for (let j=0; j<orders[i].products.length; j++) { %>
                                    <div class="good">
                                        <span class="good__name">
                                            <%= orders[i].products[j]%>
                                        </span>
                                    </div>
                                    <% } %>
                            </div>
                            <span class="panel__item total">
                                <%= "Suma: " + orders[i].price%>
                            </span>
                        </div>
                        <% } %>
                </main>
                <%- include ('components/footer.ejs') %>
    </body>

</html>